<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<DATconfig>
    <ID>1004</ID>
    <name>Data Analysis Tool</name>
	<data>   
        <sql>
            select SUM(Hours) AS Total_NS_Hours, PersonnelNo, Name, CostCenter from MUS GROUP BY PersonnelNo, Name, CostCenter order by Total_NS_Hours DESC;
        </sql> 
		<rowFormat>s|d|d|d</rowFormat>
		<sheetName>Individual nightshift in ITC</sheetName>
		<colCount>4</colCount>
    </data>
	<template>302_Excel_Tool.xls</template>
    <output>302_Excel_Tool_${endTime:yyMMdd}.xls</output>
</DATconfig>